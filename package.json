{
  "name": "potate",
  "version": "0.12.0",
  "description": "Super charged UI library with modern React API and native templates.",
  "author": "uniho",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/type.d.ts",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/type.d.ts",
      "import": "./dist/index-vite.js"
    },
    "./esbuild": {
      "types": "./dist/type.d.ts",
      "import": "./dist/index-esbuild.js"
    }
  },
  "bin": {
    "potate": "bin/potate-cli.mjs"
  },
  "scripts": {
    "build": "node ./scripts/build-core.mjs && node ./scripts/build-plugin.mjs",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uniho/potate.git"
  },
  "bugs": {
    "url": "https://github.com/uniho/potate/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "acorn": "^8.15.0",
    "acorn-jsx": "^5.3.2",
    "astring": "^1.9.0",
    "esbuild": "0.27.2"
  }
}
